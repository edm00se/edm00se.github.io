{
  "name": "edm00se.codes",
  "version": "1.0.0",
  "description": "edm00se GitHub user homepage",
  "main": "src/main.js",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/edm00se/edm00se.github.io.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "start": "npm run dev",
    "format": "prettier --write \"src/**/*.{vue,js}\"",
    "build": "npm run clean && parcel build ./src/index.html --public-url . --experimental-scope-hoisting",
    "build:ci": "npm run clean && npm run build -- --log-level 2",
    "build:sw": "rimraf dist/sw.js && node config/workbox-config.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "now-build": "npm run build:ci && cp public/404.html dist/. && npm run build:sw"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "vue": "2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-plugin-transform-vue-jsx": "^1.2.1",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.27.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "workbox-build": "^5.1.4"
  },
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0",
    "npm": ">= 3.10.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
