{
  "name": "edm00se.codes",
  "version": "1.0.0",
  "description": "edm00se GitHub user homepage",
  "main": "src/main.js",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/edm00se/edm00se.github.io.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel ./src/index.html",
    "start": "npm run dev",
    "format": "prettier --write \"src/**/*.{vue,js}\"",
    "build": "npm run clean && parcel build ./src/index.html --public-url .",
    "build:ci": "npm run clean && parcel build ./src/index.html --public-url . --log-level 2",
    "build:sw": "rimraf dist/sw.js && workbox injectManifest config/workbox-config.js",
    "test": "jest --config config/tests/jest.conf.js --coverage",
    "test:watch": "jest --config config/tests/jest.conf.js --coverage --watch",
    "now-build": "npm run build:ci && cp public/404.html dist/. && npm run build:sw"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "2.5.22"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@vue/component-compiler-utils": "2.5.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-vue-jsx": "4.0.1",
    "babel-preset-env": "^1.7.0",
    "husky": "1.3.1",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "8.1.0",
    "node-sass": "4.11.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.16.1",
    "rimraf": "2.6.3",
    "sass": "1.16.1",
    "vue-hot-reload-api": "^2.3.1",
    "vue-jest": "3.0.2",
    "vue-server-renderer": "2.5.22",
    "vue-template-compiler": "2.5.22",
    "workbox-cli": "3.6.3"
  },
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0",
    "npm": ">= 3.10.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
